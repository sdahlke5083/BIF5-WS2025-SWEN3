# Docker registry prefix (optional, e.g. "myregistry.com/", for images not hosted on dockerhub)
DOCKER_REGISTRY=

# PostgreSQL configuration
POSTGRES_DB=paperless
POSTGRES_USER=paperless
POSTGRES_PASSWORD=paperless

# Adminer configuration
ADMINER_DEFAULT_SERVER=paperless-db
ADMINER_DEFAULT_DRIVER=pgsql
ADMINER_DESIGN=dracula

# RabbitMQ configuration
RABBITMQ_DEFAULT_USER=paperless
RABBITMQ_DEFAULT_PASS=paperless

# MinIO configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# GenAI / Google Gemini configuration
GENAI_API_KEY=Test
GENAI_MODEL=gemini-2.5-pro