# Docker registry prefix (optional, e.g. "myregistry.com/", for images not hosted on dockerhub)
DOCKER_REGISTRY=

# PostgreSQL configuration
POSTGRES_DB=paperless
POSTGRES_USER=paperless
POSTGRES_PASSWORD=paperless

# Adminer configuration
ADMINER_DEFAULT_SERVER=paperless-db
ADMINER_DEFAULT_DRIVER=pgsql
ADMINER_DESIGN=dracula

# RabbitMQ configuration (defaults used by services and Dockerfiles)
# Host that services should use to reach RabbitMQ (matches container_name)
RABBITMQ_HOST=paperless-rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USER=paperless
RABBITMQ_PASS=paperless
# Optional: queue name used by OCR worker
RABBITMQ_ORC_QUEUE=ocr-queue
RABBITMQ_TASK_QUEUE=task-queue

# MinIO configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# GenAI / Google Gemini configuration
GENAI_API_KEY=Test
GENAI_MODEL=gemini-2.5-pro