{
  "ConnectionStrings": {
    "PostgresConnection": "Host=paperless-db;Port=5432;Database=paperless;Username=paperless;Password=paperless"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "NLog": {
    "autoReload": true,
    "throwConfigExceptions": false,
    "internalLogLevel": "Off",
    "variables": {
      "logDirectory": "${basedir}/logs",
      "baseLogData": "\n${longdate} [${logger}] ${uppercase:${level}}: ${message}"
    },
    "targets": {
      "errorfile": {
        "type": "File",
        "fileName": "${var:logDirectory}/Error.log",
        "layout": "${var:baseLogData} ${exception:format=tostring}",
        "keepFileOpen": false,
        "archiveEvery": "Day",
        "archiveFileName": "${var:logDirectory}/Error.{#}.log",
        "archiveNumbering": "Rolling",
        "maxArchiveFiles": 5,
        "archiveAboveSize": 20971520
      },
      "logfile": {
        "type": "File",
        "fileName": "${var:logDirectory}/Access.log",
        "layout": "${var:baseLogData}",
        "keepFileOpen": false,
        "archiveEvery": "Day",
        "archiveFileName": "${var:logDirectory}/Access.{#}.log",
        "archiveNumbering": "Rolling",
        "maxArchiveFiles": 5,
        "archiveAboveSize": 20971520
      },
      "debugfile": {
        "type": "File",
        "fileName": "${var:logDirectory}/Debug.log",
        "layout": "${var:baseLogData}",
        "keepFileOpen": false,
        "archiveEvery": "Day",
        "archiveFileName": "${var:logDirectory}/Debug.{#}.log",
        "archiveNumbering": "Rolling",
        "maxArchiveFiles": 2,
        "archiveAboveSize": 20971520
      },
      "console": {
        "type": "Console",
        "layout": "${var:baseLogData}",
        "autoFlush": true,
        "concurrentWrites": true
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "maxLevel": "Debug",
        "writeTo": "debugfile"
      },
      {
        "logger": "*",
        "minLevel": "Info",
        "maxLevel": "Warn",
        "writeTo": "logfile"
      },
      {
        "logger": "*",
        "minLevel": "Error",
        "writeTo": "errorfile"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "console"
      },
      {
        "logger": "*",
        "minLevel": "Debug",
        "writeTo": "vsdebug"
      }
    ]
  },
  "Paperless": {
    "Path": "/.data/Files",
    "Queue": {
      "ServerAddress": "paperless-rabbitmq",
      "QueueName": "demo-queue",
      "Durable": true
    }
  },
  "AllowedHosts": "*"
}
